      { name: 'Activity Logs', icon: 'fa-scroll', action: () => showPage('system-logs'), category: 'Navigate' },
      // Quick Actions
      { name: 'Chat with ULTRON', icon: 'fa-robot', action: () => { showPage('ai-chat'); setTimeout(() => selectAgent('ultron'), 100); }, category: 'Quick Action' },
      { name: 'Chat with STRIKER', icon: 'fa-briefcase', action: () => { showPage('ai-chat'); setTimeout(() => selectAgent('sales-lead'), 100); }, category: 'Quick Action' },
      { name: 'Chat with CODEX', icon: 'fa-code', action: () => { showPage('ai-chat'); setTimeout(() => selectAgent('dev-lead'), 100); }, category: 'Quick Action' },
      { name: 'Add New Lead', icon: 'fa-user-plus', action: () => { showPage('sales-crm'); setTimeout(() => openModal('add-lead'), 100); }, category: 'Quick Action' },
      { name: 'Refresh Data', icon: 'fa-sync-alt', action: () => refreshDashboard(), category: 'Quick Action' },
    ];
    
    let cmdIndex = 0;
    let filteredCmds = [...commands];
    
    // Keyboard shortcut to open
    document.addEventListener('keydown', (e) => {
      if ((e.metaKey || e.ctrlKey) && e.key === 'k') {
        e.preventDefault();
        openCommandPalette();
      }
      if (e.key === 'Escape') {
        closeCommandPalette();
      }
    });
    
    function openCommandPalette() {
      document.getElementById('commandPalette').classList.remove('hidden');
      document.getElementById('cmdInput').value = '';
      document.getElementById('cmdInput').focus();
      filteredCmds = [...commands];
      cmdIndex = 0;
      renderCommands();
    }
    
    function closeCommandPalette() {
      document.getElementById('commandPalette').classList.add('hidden');
    }
    
    function filterCommands(query) {
      const q = query.toLowerCase();
      filteredCmds = commands.filter(cmd => 
        cmd.name.toLowerCase().includes(q) || cmd.category.toLowerCase().includes(q)
      );
      cmdIndex = 0;
      renderCommands();
    }
    
    function renderCommands() {
      const container = document.getElementById('cmdResults');
      if (filteredCmds.length === 0) {
        container.innerHTML = '<p class="text-center text-gray-500 py-8">No results found</p>';
        return;
      }
      
      let html = '';
      let currentCategory = '';
      
      filteredCmds.forEach((cmd, i) => {
        if (cmd.category !== currentCategory) {
          currentCategory = cmd.category;
          html += `<div class="px-2 py-1 text-xs text-gray-500 uppercase">${currentCategory}</div>`;
        }
        html += `
          <div class="cmd-item flex items-center px-3 py-2 rounded-lg cursor-pointer ${i === cmdIndex ? 'bg-primary text-white' : 'hover:bg-gray-700'}" 
            onclick="executeCommand(${i})" onmouseenter="cmdIndex=${i}; renderCommands();">
            <i class="fas ${cmd.icon} w-5 mr-3 ${i === cmdIndex ? 'text-white' : 'text-gray-400'}"></i>
            <span>${cmd.name}</span>
          </div>
        `;
      });
      
      container.innerHTML = html;
    }
    
    function handleCmdKey(e) {
      if (e.key === 'ArrowDown') {
        e.preventDefault();
        cmdIndex = Math.min(cmdIndex + 1, filteredCmds.length - 1);
        renderCommands();
      } else if (e.key === 'ArrowUp') {
        e.preventDefault();
        cmdIndex = Math.max(cmdIndex - 1, 0);
        renderCommands();
      } else if (e.key === 'Enter') {
        e.preventDefault();
        executeCommand(cmdIndex);
      }
    }
    
    function executeCommand(index) {
      if (filteredCmds[index]) {
        filteredCmds[index].action();
        closeCommandPalette();
      }
    }
    
    // Modals
    function openModal(modalId) {
      document.getElementById('modal-' + modalId).classList.add('open');
    }
    
    function closeModal(modalId) {
      document.getElementById('modal-' + modalId).classList.remove('open');
    }
    
    // Agent Configuration
    const agentConfigs = {
      'ultron': { name: 'Ultron', role: 'Central Orchestrator', model: 'Claude Opus 4.5', color: 'indigo' },
      'sales-lead': { name: 'Sales Lead', role: 'Sales Department Head', model: 'Claude Sonnet', color: 'emerald' },
      'sales-researcher': { name: 'Lead Researcher', role: 'Find & qualify leads', model: 'Claude Sonnet', color: 'emerald' },
      'sales-email': { name: 'Email Manager', role: 'Outreach & follow-ups', model: 'Claude Sonnet', color: 'emerald' },
      'marketing-lead': { name: 'Marketing Lead', role: 'Marketing Department Head', model: 'Claude Sonnet', color: 'pink' },
      'marketing-writer': { name: 'Content Writer', role: 'Create marketing content', model: 'Claude Sonnet', color: 'pink' },
      'dev-lead': { name: 'Dev Lead', role: 'Development Department Head', model: 'Claude Opus 4.5', color: 'blue' },
      // ... more agents
    };
    
    function openAgentConfig(agentId) {
      const agent = agentConfigs[agentId] || { name: agentId, role: 'Agent', model: 'Claude Sonnet', color: 'gray' };
      
      document.getElementById('agentConfigTitle').textContent = 'Configure: ' + agent.name;
      document.getElementById('agentConfigContent').innerHTML = `
        <div class="space-y-4">
          <div>
            <label class="block text-sm text-gray-400 mb-2">Agent Name</label>
            <input type="text" value="${agent.name}" class="w-full bg-gray-700 border border-gray-600 rounded-lg px-4 py-2">
          </div>
          <div>
            <label class="block text-sm text-gray-400 mb-2">Role</label>
            <input type="text" value="${agent.role}" class="w-full bg-gray-700 border border-gray-600 rounded-lg px-4 py-2">
          </div>
          <div>
            <label class="block text-sm text-gray-400 mb-2">Model</label>
            <select class="w-full bg-gray-700 border border-gray-600 rounded-lg px-4 py-2">
              <option ${agent.model === 'Claude Opus 4.5' ? 'selected' : ''}>Claude Opus 4.5</option>
              <option ${agent.model === 'Claude Sonnet' ? 'selected' : ''}>Claude Sonnet</option>
            </select>
          </div>
          <div>
            <label class="block text-sm text-gray-400 mb-2">System Prompt</label>
            <textarea rows="4" class="w-full bg-gray-700 border border-gray-600 rounded-lg px-4 py-2">You are ${agent.name}, responsible for ${agent.role.toLowerCase()}.</textarea>
          </div>
          <div>
            <label class="block text-sm text-gray-400 mb-2">Tools Access</label>
            <div class="grid grid-cols-2 gap-2">
              <label class="flex items-center"><input type="checkbox" checked class="mr-2"> web_search</label>
              <label class="flex items-center"><input type="checkbox" checked class="mr-2"> web_fetch</label>
              <label class="flex items-center"><input type="checkbox" checked class="mr-2"> browser</label>
              <label class="flex items-center"><input type="checkbox" class="mr-2"> exec</label>
              <label class="flex items-center"><input type="checkbox" checked class="mr-2"> message</label>
              <label class="flex items-center"><input type="checkbox" class="mr-2"> cron</label>
            </div>
          </div>
          <div class="flex justify-end space-x-3 pt-4">
            <button onclick="closeModal('agent-config')" class="px-4 py-2 bg-gray-700 rounded-lg">Cancel</button>
            <button class="px-4 py-2 bg-primary rounded-lg">Save Changes</button>
          </div>
        </div>
      `;
      
      openModal('agent-config');
    }
    
    // Charts
    function initCharts() {
      // Revenue Chart
      const revenueCtx = document.getElementById('revenueChart')?.getContext('2d');
      if (revenueCtx) {
        new Chart(revenueCtx, {
          type: 'line',
          data: {
            labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun'],
            datasets: [{
              label: 'Revenue',
              data: [12000, 15000, 18000, 22000, 21000, 24500],
              borderColor: '#22c55e',
              backgroundColor: 'rgba(34, 197, 94, 0.1)',
              fill: true,
              tension: 0.4
            }]
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: { legend: { display: false } },
            scales: {
              x: { grid: { color: '#374151' }, ticks: { color: '#9ca3af' } },
              y: { grid: { color: '#374151' }, ticks: { color: '#9ca3af' } }
            }
          }
        });
      }
      
      // Activity Chart
      const activityCtx = document.getElementById('activityChart')?.getContext('2d');
      if (activityCtx) {
        new Chart(activityCtx, {
          type: 'doughnut',
          data: {
            labels: ['Sales', 'Marketing', 'Development', 'Support', 'Finance'],
            datasets: [{
              data: [30, 25, 20, 15, 10],
              backgroundColor: ['#22c55e', '#ec4899', '#3b82f6', '#06b6d4', '#eab308']
            }]
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: { legend: { position: 'bottom', labels: { color: '#9ca3af', boxWidth: 12 } } }
          }
        });
      }
      
      // Finance Chart
      const financeCtx = document.getElementById('financeChart')?.getContext('2d');
      if (financeCtx) {
        new Chart(financeCtx, {
          type: 'bar',
          data: {
            labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun'],
            datasets: [
              { label: 'Revenue', data: [12000, 15000, 18000, 22000, 21000, 24500], backgroundColor: '#22c55e' },
              { label: 'Expenses', data: [6000, 6500, 7000, 7500, 8000, 8200], backgroundColor: '#ef4444' }
            ]
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: { legend: { labels: { color: '#9ca3af' } } },
            scales: {
              x: { grid: { display: false }, ticks: { color: '#9ca3af' } },
              y: { grid: { color: '#374151' }, ticks: { color: '#9ca3af' } }
            }
          }
        });
      }
      
      // Expense Chart
      const expenseCtx = document.getElementById('expenseChart')?.getContext('2d');
      if (expenseCtx) {
        new Chart(expenseCtx, {
          type: 'doughnut',
          data: {
            labels: ['Infrastructure', 'Tools', 'Services', 'Marketing', 'Other'],
            datasets: [{
              data: [3500, 2000, 1500, 800, 400],
              backgroundColor: ['#6366f1', '#8b5cf6', '#ec4899', '#f59e0b', '#6b7280']
            }]
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: { legend: { position: 'bottom', labels: { color: '#9ca3af', boxWidth: 12 } } }
          }
        });
      }
    }
    
    // Recent Activity
    function loadRecentActivity() {
      const activities = [
        { icon: 'fa-user-plus', color: 'emerald', text: 'New lead added: TechStart Inc', time: '5m ago' },
        { icon: 'fa-envelope', color: 'blue', text: 'Email sent to 12 contacts', time: '15m ago' },
        { icon: 'fa-check', color: 'green', text: 'Test suite passed (14/14)', time: '32m ago' },
        { icon: 'fa-file-invoice', color: 'yellow', text: 'Invoice #003 created', time: '1h ago' },
        { icon: 'fa-robot', color: 'purple', text: 'Content Writer generated blog post', time: '2h ago' },
      ];
      
      const container = document.getElementById('recentActivity');
      if (container) {
        container.innerHTML = activities.map(a => `
          <div class="flex items-center p-3 bg-gray-700/30 rounded-lg">
            <div class="w-8 h-8 rounded-lg bg-${a.color}-500/20 flex items-center justify-center mr-3">
              <i class="fas ${a.icon} text-${a.color}-400 text-sm"></i>
            </div>
            <div class="flex-1">
              <p class="text-sm">${a.text}</p>
            </div>
            <span class="text-xs text-gray-500">${a.time}</span>
          </div>
        `).join('');
      }
    }
    
    // Time
    function updateTime() {
      const now = new Date();
      document.getElementById('currentTime').textContent = now.toLocaleString();
    }
    
