    // Chat state
    let currentAgent = null;
    let chatHistories = {}; // Stores chat history per agent
    
    // Load chat histories from localStorage
    function loadChatHistories() {
      const saved = localStorage.getItem('ultron-chat-histories');
      if (saved) {
        chatHistories = JSON.parse(saved);
      }
    }
    
    // Save chat histories to localStorage
    function saveChatHistories() {
      localStorage.setItem('ultron-chat-histories', JSON.stringify(chatHistories));
    }
    
    // Open agent chat (new UI)
    function openAgentChat(agentId) {
      currentAgent = agentId;
      const agent = agents[agentId];
      
      // Highlight selected agent card
      document.querySelectorAll('.agent-card').forEach(el => {
        el.classList.remove('ring-2', 'ring-primary');
      });
      event.target.closest('.agent-card')?.classList.add('ring-2', 'ring-primary');
      
      // Show chat area
      document.getElementById('noChatSelected').classList.add('hidden');
      document.getElementById('activeChat').classList.remove('hidden');
      
      // Update header with emoji
      document.getElementById('chatAgentEmoji').textContent = agent.emoji;
      document.getElementById('chatAgentName').textContent = agent.name;
      document.getElementById('chatAgentRole').textContent = agent.role;
      
      // Load chat history
      loadChatHistory(agentId);
      
      // Focus input
      document.getElementById('chatInput').focus();
    }
    
    // Close agent chat
    function closeAgentChat() {
      currentAgent = null;
      document.getElementById('noChatSelected').classList.remove('hidden');
      document.getElementById('activeChat').classList.add('hidden');
      document.querySelectorAll('.agent-card').forEach(el => {
        el.classList.remove('ring-2', 'ring-primary');
      });
    }
    
    // Legacy support
    function selectAgent(agentId) {
      openAgentChat(agentId);
    }
    
    // Load chat history for an agent
    function loadChatHistory(agentId) {
      const container = document.getElementById('chatMessages');
      const history = chatHistories[agentId] || [];
      const agent = agents[agentId];
      
      if (history.length === 0) {
        // Show welcome message
        container.innerHTML = `
          <div class="flex items-start">
            <div class="text-2xl mr-3">${agent.emoji}</div>
            <div class="bg-gray-700 rounded-2xl rounded-tl-sm px-4 py-3 max-w-[80%]">
              <p class="text-sm">Hey! I'm ${agent.name} ${agent.emoji}. How can I help you today?</p>
            </div>
          </div>
        `;
      } else {
        container.innerHTML = history.map(msg => renderMessage(msg)).join('');
      }
      
      // Scroll to bottom
      container.scrollTop = container.scrollHeight;
    }
    
    // Render a message
    function renderMessage(msg) {
      const agent = agents[currentAgent];
      
      if (msg.role === 'user') {
        return `
          <div class="flex items-start justify-end">
            <div class="bg-primary rounded-2xl rounded-tr-sm px-4 py-3 max-w-[80%]">
              <p class="text-sm">${escapeHtml(msg.content)}</p>
            </div>
          </div>
        `;
      } else {
        return `
          <div class="flex items-start">
            <div class="text-2xl mr-3">${agent.emoji}</div>
            <div class="bg-gray-700 rounded-2xl rounded-tl-sm px-4 py-3 max-w-[80%]">
              <p class="text-sm">${escapeHtml(msg.content)}</p>
            </div>
          </div>
        `;
      }
    }
    
    // Escape HTML
    function escapeHtml(text) {
      const div = document.createElement('div');
      div.textContent = text;
      return div.innerHTML.replace(/\n/g, '<br>');
    }
    
    // Send message
    async function sendMessage() {
      const input = document.getElementById('chatInput');
      const message = input.value.trim();
      
      if (!message || !currentAgent) return;
      
      // Clear input
      input.value = '';
      input.style.height = 'auto';
      
      // Add user message
      if (!chatHistories[currentAgent]) chatHistories[currentAgent] = [];
      chatHistories[currentAgent].push({ role: 'user', content: message });
      
      // Render message
      const container = document.getElementById('chatMessages');
      container.innerHTML += renderMessage({ role: 'user', content: message });
      
      // Add typing indicator
      container.innerHTML += `
        <div id="typingIndicator" class="flex items-start">
          <div class="text-2xl mr-3">${agents[currentAgent].emoji}</div>
          <div class="bg-gray-700 rounded-2xl rounded-tl-sm px-4 py-3">
            <div class="flex space-x-1">
              <div class="w-2 h-2 bg-gray-500 rounded-full animate-bounce" style="animation-delay: 0ms"></div>
              <div class="w-2 h-2 bg-gray-500 rounded-full animate-bounce" style="animation-delay: 150ms"></div>
              <div class="w-2 h-2 bg-gray-500 rounded-full animate-bounce" style="animation-delay: 300ms"></div>
            </div>
          </div>
        </div>
      `;
      container.scrollTop = container.scrollHeight;
      
      // Get AI response
      try {
        const response = await fetch('/api/chat', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({
            agent: currentAgent,
            message: message,
            history: chatHistories[currentAgent].slice(-10) // Last 10 messages for context
          })
        });
        
        const data = await response.json();
        
        // Remove typing indicator
        document.getElementById('typingIndicator')?.remove();
        
        // Add assistant response
        const reply = data.reply || "I'm sorry, I couldn't process that request. Please try again.";
        chatHistories[currentAgent].push({ role: 'assistant', content: reply });
        container.innerHTML += renderMessage({ role: 'assistant', content: reply });
        
        // Save and scroll
        saveChatHistories();
        container.scrollTop = container.scrollHeight;
        
      } catch (error) {
        document.getElementById('typingIndicator')?.remove();
        
        // Fallback response
        const fallback = generateFallbackResponse(currentAgent, message);
        chatHistories[currentAgent].push({ role: 'assistant', content: fallback });
        container.innerHTML += renderMessage({ role: 'assistant', content: fallback });
        saveChatHistories();
        container.scrollTop = container.scrollHeight;
      }
    }
    
    // Generate fallback response (when API not available)
    function generateFallbackResponse(agentId, message) {
      const agent = agents[agentId];
      const responses = {
        'ultron': [
          "I'm analyzing the situation and will coordinate with the relevant departments.",
          "Let me check the status across all systems and get back to you.",
          "I've noted your request. I'll ensure the right team handles this efficiently."
        ],
        'sales-lead': [
          "That's a great point for our sales strategy. Let me review our pipeline data.",
          "I'll have the team look into this lead opportunity right away.",
          "Our conversion metrics suggest we should focus on this approach."
        ],
        'marketing-lead': [
          "Interesting marketing angle! Let me think about how we can leverage this.",
          "I'll add this to our content calendar and brief the team.",
          "Our brand guidelines align well with this direction."
        ],
        'dev-lead': [
          "I'll review the technical requirements and scope this out.",
          "Let me check our sprint capacity and see when we can tackle this.",
          "Good idea - I'll create a ticket and prioritize accordingly."
        ],
        'finance-lead': [
          "I'll run the numbers and prepare a detailed analysis.",
          "Let me check our budget allocation for this quarter.",
          "The financial impact looks manageable. I'll prepare a report."
        ]
      };
      
      const agentResponses = responses[agentId] || [
        `Thanks for reaching out! As ${agent.role}, I'll look into this for you.`,
        `I understand. Let me work on this and get back to you with updates.`,
        `Great question! I'll coordinate with the team on this.`
      ];
      
      return agentResponses[Math.floor(Math.random() * agentResponses.length)];
    }
    
    // Clear chat
    function clearChat() {
      if (!currentAgent) return;
      if (confirm('Clear chat history with ' + agents[currentAgent].name + '?')) {
        chatHistories[currentAgent] = [];
        saveChatHistories();
        loadChatHistory(currentAgent);
      }
    }
    
    // Handle Enter key
    function handleChatKeydown(event) {
      if (event.key === 'Enter' && !event.shiftKey) {
        event.preventDefault();
        sendMessage();
      }
    }
    
    // Auto-resize textarea
    function autoResizeTextarea(el) {
      el.style.height = 'auto';
      el.style.height = Math.min(el.scrollHeight, 128) + 'px';
    }
    
    // ===================== CRM SYSTEM =====================
    
    let allLeads = [];
    let filteredLeads = [];
    let currentPage = 1;
    let currentStageFilter = 'all';
    const leadsPerPage = 25;
    
    // Load leads from API
    async function loadLeads() {
      try {
        const response = await fetch('/api/leads');
        const data = await response.json();
        allLeads = data.leads || [];
        filteredLeads = [...allLeads];
        updateStats();
        renderLeads();
        document.getElementById('crmLeadCount').textContent = `${allLeads.length} leads in pipeline`;
      } catch (error) {
        console.error('Failed to load leads:', error);
      }
    }
    
    // Update pipeline stats
    function updateStats() {
      const counts = { all: allLeads.length };
      ['new', 'contacted', 'qualified', 'proposal', 'negotiation', 'won', 'lost'].forEach(stage => {
        counts[stage] = allLeads.filter(l => l.stage === stage).length;
      });
      
      Object.entries(counts).forEach(([stage, count]) => {
        const el = document.getElementById('stat-' + stage);
        if (el) el.textContent = count;
      });
    }
    
    // Filter by stage
    function filterByStage(stage) {
      currentStageFilter = stage;
      currentPage = 1;
      filterLeads();
      
      // Update active state
      document.querySelectorAll('#crmStats > div').forEach(el => {
        el.classList.remove('border-primary', 'bg-primary/10');
      });
      event.target.closest('div').classList.add('border-primary', 'bg-primary/10');
    }
    
    // Filter leads
    function filterLeads() {
      const search = document.getElementById('crmSearch')?.value.toLowerCase() || '';
      const source = document.getElementById('filterSource')?.value || '';
      const tag = document.getElementById('filterTag')?.value || '';
      
      filteredLeads = allLeads.filter(lead => {
        // Stage filter
        if (currentStageFilter !== 'all' && lead.stage !== currentStageFilter) return false;
        
        // Search filter
        if (search) {
          const searchFields = [lead.company, lead.email, lead.notes, lead.website, lead.otherContacts].join(' ').toLowerCase();
          if (!searchFields.includes(search)) return false;
        }
        
        // Source filter
        if (source && lead.source !== source) return false;
        
        // Tag filter
        if (tag) {
          const notesLower = (lead.notes || '').toLowerCase();
          if (tag === 'No SDK' && !notesLower.includes('no sdk') && !notesLower.includes('dont have sdk')) return false;
          else if (tag !== 'No SDK' && !notesLower.includes(tag.toLowerCase())) return false;
        }
        
        return true;
      });
      
      currentPage = 1;
      renderLeads();
    }
    
    // Render leads table
    function renderLeads() {
      const start = (currentPage - 1) * leadsPerPage;
      const end = start + leadsPerPage;
      const pageLeads = filteredLeads.slice(start, end);
      
      const stageColors = {
        new: 'gray', contacted: 'cyan', qualified: 'blue', 
        proposal: 'yellow', negotiation: 'purple', won: 'green', lost: 'red'
      };
      
      const tbody = document.getElementById('crmTableBody');
      tbody.innerHTML = pageLeads.map(lead => `
        <tr class="hover:bg-gray-700/50 cursor-pointer" onclick="openLeadDetail('${lead.id}')">
          <td class="px-4 py-3">
            <div class="flex items-center">
              <div class="w-8 h-8 rounded-lg bg-gray-700 flex items-center justify-center mr-3">
                <span class="text-xs font-bold">${(lead.company || 'U')[0].toUpperCase()}</span>
              </div>
              <div>
                <p class="font-medium">${escapeHtml(lead.company || 'Unknown')}</p>
                ${lead.website ? `<a href="${lead.website}" target="_blank" class="text-xs text-primary hover:underline" onclick="event.stopPropagation()">${lead.website.replace('https://', '').replace('http://', '')}</a>` : ''}
              </div>
            </div>
          </td>
          <td class="px-4 py-3 hidden md:table-cell">
            <span class="text-sm text-gray-400">${escapeHtml(lead.email || '-')}</span>
          </td>
          <td class="px-4 py-3">
            <span class="px-2 py-1 text-xs bg-${stageColors[lead.stage] || 'gray'}-500/20 text-${stageColors[lead.stage] || 'gray'}-400 rounded capitalize">${lead.stage}</span>
          </td>
          <td class="px-4 py-3 hidden lg:table-cell">
            <p class="text-sm text-gray-400 truncate max-w-xs">${escapeHtml(lead.notes || '-')}</p>
          </td>
          <td class="px-4 py-3">
            <div class="flex items-center space-x-2">
              ${lead.email ? `<a href="mailto:${lead.email}" onclick="event.stopPropagation()" class="p-1.5 text-gray-400 hover:text-white hover:bg-gray-700 rounded" title="Email"><i class="fas fa-envelope text-xs"></i></a>` : ''}
              ${lead.website ? `<a href="${lead.website}" target="_blank" onclick="event.stopPropagation()" class="p-1.5 text-gray-400 hover:text-white hover:bg-gray-700 rounded" title="Website"><i class="fas fa-external-link text-xs"></i></a>` : ''}
              <button onclick="event.stopPropagation(); updateLeadStage('${lead.id}')" class="p-1.5 text-gray-400 hover:text-white hover:bg-gray-700 rounded" title="Update Stage"><i class="fas fa-arrow-right text-xs"></i></button>
            </div>
          </td>
        </tr>
      `).join('');
      
      // Update pagination
      document.getElementById('crmPagination').textContent = `Showing ${start + 1}-${Math.min(end, filteredLeads.length)} of ${filteredLeads.length} leads`;
      document.getElementById('pageInfo').textContent = `Page ${currentPage} of ${Math.ceil(filteredLeads.length / leadsPerPage) || 1}`;
      document.getElementById('prevBtn').disabled = currentPage <= 1;
      document.getElementById('nextBtn').disabled = end >= filteredLeads.length;
    }
    
    // Pagination
    function prevPage() {
      if (currentPage > 1) {
        currentPage--;
        renderLeads();
      }
    }
    
    function nextPage() {
      if (currentPage * leadsPerPage < filteredLeads.length) {
        currentPage++;
        renderLeads();
      }
    }
    
    // Open lead detail
    function openLeadDetail(leadId) {
      const lead = allLeads.find(l => l.id === leadId);
      if (!lead) return;
      
      document.getElementById('leadDetailTitle').textContent = lead.company || 'Unknown Company';
      document.getElementById('leadDetailSubtitle').textContent = lead.website?.replace('https://', '') || 'No website';
      
      const stageColors = {
        new: 'gray', contacted: 'cyan', qualified: 'blue', 
        proposal: 'yellow', negotiation: 'purple', won: 'green', lost: 'red'
      };
      const color = stageColors[lead.stage] || 'gray';
      
      document.getElementById('leadDetailContent').innerHTML = `
        <div class="space-y-6">
          <!-- Quick Actions -->
          <div class="flex flex-wrap gap-2">
            ${lead.email ? `<a href="mailto:${lead.email}" class="px-3 py-1.5 bg-emerald-500/20 text-emerald-400 rounded-lg text-sm hover:bg-emerald-500/30"><i class="fas fa-envelope mr-2"></i>Send Email</a>` : ''}
            ${lead.website ? `<a href="${lead.website}" target="_blank" class="px-3 py-1.5 bg-blue-500/20 text-blue-400 rounded-lg text-sm hover:bg-blue-500/30"><i class="fas fa-globe mr-2"></i>Visit Website</a>` : ''}
            <button onclick="updateLeadStageModal('${lead.id}')" class="px-3 py-1.5 bg-purple-500/20 text-purple-400 rounded-lg text-sm hover:bg-purple-500/30"><i class="fas fa-arrow-right mr-2"></i>Move Stage</button>
          </div>
          
          <!-- Info Grid -->
          <div class="grid grid-cols-1 md:grid-cols-2 gap-4">
            <div class="bg-gray-700/50 rounded-lg p-4">
              <p class="text-xs text-gray-500 uppercase mb-1">Status</p>
              <span class="px-2 py-1 text-sm bg-${color}-500/20 text-${color}-400 rounded capitalize">${lead.stage}</span>
            </div>
            <div class="bg-gray-700/50 rounded-lg p-4">
              <p class="text-xs text-gray-500 uppercase mb-1">Source</p>
              <p class="text-sm">${lead.source || 'Unknown'}</p>
            </div>
            <div class="bg-gray-700/50 rounded-lg p-4">
              <p class="text-xs text-gray-500 uppercase mb-1">Email</p>
              <p class="text-sm">${lead.email || '-'}</p>
            </div>
            <div class="bg-gray-700/50 rounded-lg p-4">
              <p class="text-xs text-gray-500 uppercase mb-1">Other Contacts</p>
              <p class="text-sm">${lead.otherContacts || '-'}</p>
            </div>
          </div>
          
          <!-- Notes -->
          <div class="bg-gray-700/50 rounded-lg p-4">
            <p class="text-xs text-gray-500 uppercase mb-2">Notes</p>
            <p class="text-sm whitespace-pre-wrap">${escapeHtml(lead.notes || 'No notes yet.')}</p>
          </div>
          
          <!-- Conversation History -->
          <div>
            <div class="flex items-center justify-between mb-3">
              <h4 class="font-semibold text-gray-300"><i class="fas fa-comments mr-2"></i>Conversation History</h4>
              <button onclick="addConversation('${lead.id}')" class="text-xs text-primary hover:underline">+ Add Note</button>
            </div>
            <div class="space-y-3" id="conversationList-${lead.id}">
              ${(lead.conversations || []).length === 0 
                ? '<p class="text-sm text-gray-500 italic">No conversations recorded yet.</p>'
                : (lead.conversations || []).map(c => `
                  <div class="bg-gray-700/30 rounded-lg p-3 border-l-2 border-${c.type === 'email' ? 'emerald' : c.type === 'call' ? 'blue' : 'gray'}-500">
                    <div class="flex items-center justify-between mb-1">
                      <span class="text-xs text-gray-400"><i class="fas fa-${c.type === 'email' ? 'envelope' : c.type === 'call' ? 'phone' : 'sticky-note'} mr-1"></i>${c.type}</span>
                      <span class="text-xs text-gray-500">${c.date}</span>
                    </div>
                    <p class="text-sm">${escapeHtml(c.content)}</p>
                  </div>
                `).join('')
              }
            </div>
          </div>
          
          <!-- Tags -->
          <div>
            <p class="text-xs text-gray-500 uppercase mb-2">Tags</p>
            <div class="flex flex-wrap gap-2" id="leadTags-${lead.id}">
              ${(lead.tags || []).map(tag => `
                <span class="px-2 py-1 bg-primary/20 text-primary text-xs rounded">${tag}</span>
              `).join('') || '<span class="text-sm text-gray-500 italic">No tags</span>'}
              <button onclick="addTag('${lead.id}')" class="px-2 py-1 border border-dashed border-gray-600 text-gray-400 text-xs rounded hover:border-gray-400">+ Add Tag</button>
            </div>
          </div>
        </div>
      `;
      
      openModal('lead-detail');
    }
    
    // Update lead stage
    function updateLeadStage(leadId) {
      const lead = allLeads.find(l => l.id === leadId);
      if (!lead) return;
      
      const stages = ['new', 'contacted', 'qualified', 'proposal', 'negotiation', 'won'];
      const currentIndex = stages.indexOf(lead.stage);
      const nextStage = stages[Math.min(currentIndex + 1, stages.length - 1)];
      
      lead.stage = nextStage;
      updateStats();
      filterLeads();
      saveCrmData();
    }
    
    // Save CRM data (local for now)
    function saveCrmData() {
      localStorage.setItem('ultron-crm-leads', JSON.stringify(allLeads));
    }
    
    // Add new lead
    function addNewLead(event) {
      event.preventDefault();
      
      const newLead = {
        id: 'lead-' + Date.now(),
        company: document.getElementById('newLeadCompany').value,
        website: document.getElementById('newLeadWebsite').value,
        email: document.getElementById('newLeadEmail').value,
        stage: document.getElementById('newLeadStage').value,
        notes: document.getElementById('newLeadNotes').value,
        source: 'Manual',
        tags: [],
        conversations: [],
        createdAt: new Date().toISOString().split('T')[0]
      };
      
      allLeads.unshift(newLead);
      updateStats();
      filterLeads();
      saveCrmData();
      closeModal('add-lead');
      
      // Clear form
      document.getElementById('newLeadCompany').value = '';
      document.getElementById('newLeadWebsite').value = '';
      document.getElementById('newLeadEmail').value = '';
      document.getElementById('newLeadNotes').value = '';
    }
    
    // Export leads
    function exportLeads() {
      const csv = [
        ['Company', 'Website', 'Email', 'Stage', 'Source', 'Notes'].join(','),
        ...filteredLeads.map(l => [
          `"${(l.company || '').replace(/"/g, '""')}"`,
          l.website || '',
          l.email || '',
          l.stage || '',
          l.source || '',
          `"${(l.notes || '').replace(/"/g, '""')}"`
        ].join(','))
      ].join('\n');
      
      const blob = new Blob([csv], { type: 'text/csv' });
      const url = URL.createObjectURL(blob);
      const a = document.createElement('a');
      a.href = url;
      a.download = 'leads-export-' + new Date().toISOString().split('T')[0] + '.csv';
      a.click();
    }
    
    // ===================== CLIENT RESULTS =====================
    
    let clientData = null;
    
    async function loadClientData() {
      try {
        const response = await fetch('/api/finance');
        clientData = await response.json();
        renderClientData();
      } catch (error) {
        console.error('Failed to load client data:', error);
      }
    }
    
    function renderClientData() {
      if (!clientData) return;
      
      // Update last updated
      document.getElementById('clientsLastUpdated').textContent = 
        clientData.lastUpdated ? `Updated: ${clientData.lastUpdated}` : 'Live';
      
      // Format helpers
      const fmt = (n) => '$' + (n || 0).toLocaleString('en-US', {maximumFractionDigits: 0});
      const fmtK = (n) => (n || 0).toLocaleString('en-US');
      
      // Total stats
      document.getElementById('clientsTotalRev').textContent = fmt(clientData.overview?.totalRevenue);
      document.getElementById('clientsTotalPaid').textContent = fmt(clientData.overview?.totalPaid);
      document.getElementById('clientsTotalSdk').textContent = fmtK(clientData.infrastructure?.sdkTotal);
      document.getElementById('clientsWeekly').textContent = fmt(clientData.overview?.weeklyEarnings);
      
      // Device stats
      document.getElementById('clientsWindows').textContent = fmtK(clientData.infrastructure?.windowsOnline);
      document.getElementById('clientsMac').textContent = fmtK(clientData.infrastructure?.macOnline);
      document.getElementById('clientsAndroid').textContent = fmtK(clientData.infrastructure?.androidOnline);
      document.getElementById('clientsAndroidNew').textContent = '+' + fmtK(clientData.infrastructure?.androidNew);
      
      // Top countries
      const winTop = (clientData.infrastructure?.winTop5 || []).slice(0, 3).map(c => c.country).join(', ');
      const macTop = (clientData.infrastructure?.macTop5 || []).slice(0, 3).map(c => c.country).join(', ');
      document.getElementById('clientsWinTop').textContent = winTop || 'N/A';
      document.getElementById('clientsMacTop').textContent = macTop || 'N/A';
      
      // BigMama
      document.getElementById('bmTotal').textContent = fmtK(clientData.infrastructure?.bigmamaTotal);
      document.getElementById('bmOnline').textContent = fmtK(clientData.infrastructure?.bigmamaOnline || 15584);
      document.getElementById('bmIncome').textContent = fmt(clientData.overview?.totalRevenue);
      document.getElementById('bmPaid').textContent = fmt(clientData.overview?.totalPaid);
      document.getElementById('bmPending').textContent = fmt(clientData.overview?.pending);
      
      renderClientTable();
    }
    
    function renderClientTable() {
      if (!clientData || !clientData.partners) return;
      
      const sortBy = document.getElementById('clientSort')?.value || 'nodes';
      let partners = [...clientData.partners];
      
      if (sortBy === 'nodes') {
        partners.sort((a, b) => (b.nodes || 0) - (a.nodes || 0));
      } else {
        partners.sort((a, b) => (a.name || '').localeCompare(b.name || ''));
      }
      
      const total = partners.reduce((sum, p) => sum + (p.nodes || 0), 0);
      
      const colors = {
        'HoneyGain': 'yellow', 'BarakTv': 'blue', 'SOAX': 'green', 'Soax': 'green',
        'PacketSDK': 'purple', 'Infatica': 'cyan', 'BigMama': 'pink',
        'ProxiesGG': 'orange', 'IPGoGo': 'gray'
      };
      
      document.getElementById('clientTableBody').innerHTML = partners.map(p => {
        const share = total > 0 ? ((p.nodes / total) * 100).toFixed(1) : 0;
        const color = colors[p.name] || 'gray';
        return `
          <tr class="hover:bg-gray-700/50">
            <td class="px-6 py-4">
              <div class="flex items-center">
                <div class="w-10 h-10 rounded-lg bg-${color}-500/20 flex items-center justify-center mr-3">
                  <span class="text-${color}-400 font-bold">${(p.name || 'U')[0]}</span>
                </div>
                <div>
                  <p class="font-medium">${p.name}</p>
                  <p class="text-xs text-gray-500">SDK Partner</p>
                </div>
              </div>
            </td>
            <td class="px-6 py-4">
              <span class="text-lg font-bold">${(p.nodes || 0).toLocaleString()}</span>
            </td>
            <td class="px-6 py-4">
              <div class="flex items-center">
                <div class="w-20 bg-gray-700 rounded-full h-2 mr-2">
                  <div class="bg-${color}-500 h-2 rounded-full" style="width: ${share}%"></div>
                </div>
                <span class="text-sm">${share}%</span>
              </div>
            </td>
            <td class="px-6 py-4">
              <span class="px-2 py-1 text-xs bg-green-500/20 text-green-400 rounded">Active</span>
            </td>
            <td class="px-6 py-4">
              <span class="text-green-400 text-sm"><i class="fas fa-arrow-up mr-1"></i>Stable</span>
            </td>
          </tr>
        `;
      }).join('');
    }
    
    // ===================== DASHBOARD SUMMARY =====================
    
    async function loadDashboardData() {
      // Load finance data for dashboard
      try {
        const finRes = await fetch('/api/finance');
        const finData = await finRes.json();
        if (finData.overview) {
          document.getElementById('dashWeeklyRev').textContent = '$' + (finData.overview.weeklyEarnings/1000).toFixed(1) + 'K';
          document.getElementById('dashPending').textContent = '$' + (finData.overview.pending/1000).toFixed(1) + 'K';
        }
        if (finData.infrastructure) {
          document.getElementById('dashDesktop').textContent = (finData.infrastructure.desktopOnline/1000).toFixed(0) + 'K';
          document.getElementById('dashAndroid').textContent = (finData.infrastructure.androidOnline/1000).toFixed(0) + 'K';
          document.getElementById('dashSdk').textContent = (finData.infrastructure.sdkTotal/1000).toFixed(0) + 'K';
          document.getElementById('dashBigmama').textContent = (finData.infrastructure.bigmamaTotal/1000).toFixed(0) + 'K';
        }
      } catch (e) { console.error('Dashboard finance:', e); }
      
      // Load test results for dashboard
      try {
        const testRes = await fetch('/api/results');
        const testData = await testRes.json();
        const latest = testData.latest || {};
        const tests = Object.entries(latest);
        const passed = tests.filter(([_, t]) => t.passed).length;
        const failed = tests.filter(([_, t]) => !t.passed).length;
        
        document.getElementById('dashTestsPassed').textContent = passed;
        document.getElementById('dashTestsFailed').textContent = failed;
        
        // Show last 4 tests
        const container = document.getElementById('dashTestSummary');
        container.innerHTML = tests.slice(0, 4).map(([name, result]) => {
          const status = result.passed;
          const color = status ? 'green' : 'red';
          const icon = status ? 'check' : 'times';
          return `
            <div class="bg-gray-700/50 rounded-lg p-2 text-center">
              <i class="fas fa-${icon} text-${color}-400 mb-1"></i>
              <p class="text-xs truncate">${name.replace(/-/g, ' ')}</p>
            </div>
          `;
        }).join('');
      } catch (e) { console.error('Dashboard tests:', e); }
      
      // Load leads count
      try {
        const leadsRes = await fetch('/api/leads');
        const leadsData = await leadsRes.json();
        document.getElementById('dashLeadsCount').textContent = (leadsData.leads || []).length;
      } catch (e) { console.error('Dashboard leads:', e); }
    }
    
    function refreshDashboard() {
      loadDashboardData();
      loadFinanceData();
      loadTestResults();
      loadLeads();
    }
    
    // ===================== TESTING SYSTEM =====================
    
    let testResults = null;
    
    async function loadTestResults() {
      try {
        const response = await fetch('/api/results');
        testResults = await response.json();
        renderTestResults();
      } catch (error) {
        console.error('Failed to load test results:', error);
      }
    }
    
    function renderTestResults() {
      if (!testResults) return;
      
      const latest = testResults.latest || {};
      const tests = Object.entries(latest);
      
      // Stats
      const passed = tests.filter(([_, t]) => t.passed).length;
      const failed = tests.filter(([_, t]) => !t.passed).length;
      
      document.getElementById('testsPassed').textContent = passed;
      document.getElementById('testsFailed').textContent = failed;
      document.getElementById('testsTotal').textContent = tests.length;
      
      // Last run time
      if (tests.length > 0) {
        const lastTest = tests.sort((a, b) => new Date(b[1].timestamp) - new Date(a[1].timestamp))[0];
        const lastTime = new Date(lastTest[1].timestamp);
        const minutesAgo = Math.floor((Date.now() - lastTime) / 60000);
        document.getElementById('testsLastRun').textContent = minutesAgo < 60 ? `${minutesAgo}m ago` : `${Math.floor(minutesAgo/60)}h ago`;
      }
      
      // Test cards
      const grid = document.getElementById('testResultsGrid');
      grid.innerHTML = tests.map(([name, result]) => {
        const status = result.passed;
        const color = status ? 'green' : 'red';
        const icon = status ? 'check-circle' : 'times-circle';
        const duration = result.duration ? (result.duration / 1000).toFixed(1) + 's' : '-';
        const time = result.timestamp ? new Date(result.timestamp).toLocaleTimeString() : '-';
        
        // Summary details
        let details = [];
        if (result.summary) {
          if (result.summary.totalRequests !== undefined) details.push(`${result.summary.totalRequests} requests`);
          if (result.summary.failed !== undefined) details.push(`${result.summary.failed} failed`);
          if (result.summary.errors !== undefined) details.push(`${result.summary.errors} errors`);
          if (result.summary.throughput !== undefined) details.push(`${result.summary.throughput.toFixed(1)} req/s`);
          if (result.summary.uniqueIps !== undefined) details.push(`${result.summary.uniqueIps} IPs`);
        }
        
        return `
          <div class="bg-gray-800 rounded-xl p-4 border border-${color}-500/30 hover:border-${color}-500/50 transition">
            <div class="flex items-center justify-between mb-3">
              <div class="flex items-center">
                <i class="fas fa-${icon} text-${color}-400 mr-2"></i>
                <span class="font-medium capitalize">${name.replace(/-/g, ' ')}</span>
              </div>
              <span class="text-xs text-gray-500">${time}</span>
            </div>
            <div class="flex items-center justify-between text-sm">
              <span class="text-gray-400">Duration: ${duration}</span>
              <span class="px-2 py-0.5 text-xs bg-${color}-500/20 text-${color}-400 rounded">${status ? 'PASS' : 'FAIL'}</span>
            </div>
            ${details.length > 0 ? `
              <div class="mt-2 pt-2 border-t border-gray-700">
                <p class="text-xs text-gray-500">${details.join(' • ')}</p>
              </div>
            ` : ''}
          </div>
        `;
      }).join('');
      
      // Schedules
      const schedules = testResults.schedule || {};
      const scheduleContainer = document.getElementById('testSchedules');
      scheduleContainer.innerHTML = Object.entries(schedules).map(([name, sched]) => {
        const nextIn = sched.inMinutes || 0;
        return `
          <div class="bg-gray-700/50 rounded-lg p-3">
            <div class="flex items-center justify-between mb-1">
              <span class="font-medium capitalize text-sm">${name}</span>
              <span class="text-xs text-gray-500">${sched.tests || 0} tests</span>
            </div>
            <p class="text-xs text-gray-400 mb-2">${sched.description || ''}</p>
            <div class="flex items-center text-xs">
              <i class="fas fa-clock text-yellow-400 mr-1"></i>
              <span class="text-yellow-400">Next: ${nextIn}m</span>
            </div>
          </div>
        `;
      }).join('');
    }
    
    // ===================== FINANCE SYSTEM =====================
    
    let financeData = null;
    let partnerChart = null;
    let platformChart = null;
    
    async function loadFinanceData() {
      try {
        const response = await fetch('/api/finance');
        financeData = await response.json();
        renderFinanceData();
      } catch (error) {
        console.error('Failed to load finance data:', error);
      }
    }
    
    function renderFinanceData() {
      if (!financeData) return;
      
      // Update last updated
      document.getElementById('financeLastUpdated').textContent = 
        financeData.lastUpdated ? `Updated: ${financeData.lastUpdated}` : 'Live';
      
      // Overview stats
      const fmt = (n) => '$' + (n || 0).toLocaleString('en-US', {maximumFractionDigits: 0});
      const fmtK = (n) => (n || 0).toLocaleString('en-US');
      
      document.getElementById('finTotalRevenue').textContent = fmt(financeData.overview?.totalRevenue);
      document.getElementById('finTotalPaid').textContent = fmt(financeData.overview?.totalPaid);
      document.getElementById('finWeekly').textContent = fmt(financeData.overview?.weeklyEarnings);
      document.getElementById('finPending').textContent = fmt(financeData.overview?.pending);
      
      // Infrastructure
      document.getElementById('finDesktopOnline').textContent = fmtK(financeData.infrastructure?.desktopOnline);
      document.getElementById('finAndroidOnline').textContent = fmtK(financeData.infrastructure?.androidOnline);
      document.getElementById('finSdkTotal').textContent = fmtK(financeData.infrastructure?.sdkTotal);
      document.getElementById('finBigmamaTotal').textContent = fmtK(financeData.infrastructure?.bigmamaTotal);
      
      // Partner table
      const partners = financeData.partners || [];
      const totalNodes = partners.reduce((sum, p) => sum + (p.nodes || 0), 0);
      
      document.getElementById('partnerTableBody').innerHTML = partners.map(p => {
        const share = totalNodes > 0 ? ((p.nodes / totalNodes) * 100).toFixed(1) : 0;
        const colors = {
          'HoneyGain': 'yellow', 'BarakTv': 'blue', 'SOAX': 'green', 'Soax': 'green',
          'PacketSDK': 'purple', 'Infatica': 'cyan', 'BigMama': 'pink',
          'ProxiesGG': 'orange', 'IPGoGo': 'gray'
        };
        const color = colors[p.name] || 'gray';
        return `
          <tr class="hover:bg-gray-700/50">
            <td class="px-6 py-4">
              <div class="flex items-center">
                <div class="w-8 h-8 rounded-lg bg-${color}-500/20 flex items-center justify-center mr-3">
                  <span class="text-${color}-400 text-xs font-bold">${p.name[0]}</span>
                </div>
                <span class="font-medium">${p.name}</span>
              </div>
            </td>
            <td class="px-6 py-4 font-medium">${fmtK(p.nodes)}</td>
            <td class="px-6 py-4">
              <div class="flex items-center">
                <div class="w-16 bg-gray-700 rounded-full h-2 mr-2">
                  <div class="bg-${color}-500 h-2 rounded-full" style="width: ${share}%"></div>
                </div>
                <span class="text-sm text-gray-400">${share}%</span>
              </div>
            </td>
            <td class="px-6 py-4">
              <span class="px-2 py-1 text-xs bg-green-500/20 text-green-400 rounded">Active</span>
            </td>
          </tr>
        `;
      }).join('');
      
      // Update charts
      updateFinanceCharts(partners);
    }
    
    function updateFinanceCharts(partners) {
      const colors = ['#eab308', '#3b82f6', '#22c55e', '#a855f7', '#06b6d4', '#ec4899', '#f97316', '#6b7280'];
      
      // Partner chart
      const partnerCtx = document.getElementById('financeChart')?.getContext('2d');
      if (partnerCtx) {
        if (partnerChart) partnerChart.destroy();
        partnerChart = new Chart(partnerCtx, {
          type: 'doughnut',
          data: {
            labels: partners.map(p => p.name),
            datasets: [{
              data: partners.map(p => p.nodes),
              backgroundColor: colors.slice(0, partners.length)
            }]
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: { legend: { position: 'bottom', labels: { color: '#9ca3af', boxWidth: 12 } } }
          }
        });
      }
      
      // Platform chart
      const platformCtx = document.getElementById('expenseChart')?.getContext('2d');
      if (platformCtx && financeData.infrastructure) {
        if (platformChart) platformChart.destroy();
        platformChart = new Chart(platformCtx, {
          type: 'doughnut',
          data: {
            labels: ['Windows', 'Mac', 'Android'],
            datasets: [{
              data: [
                financeData.infrastructure.windowsOnline,
                financeData.infrastructure.macOnline,
                financeData.infrastructure.androidOnline
              ],
              backgroundColor: ['#3b82f6', '#a855f7', '#22c55e']
            }]
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: { legend: { position: 'bottom', labels: { color: '#9ca3af', boxWidth: 12 } } }
          }
        });
      }
    }
    
