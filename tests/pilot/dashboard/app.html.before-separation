      <!-- Meeting Header -->
      <div class="bg-gradient-to-r from-purple-900/30 to-blue-900/30 rounded-xl border border-purple-500/30 p-5 mb-6">
        <div class="flex items-center gap-3 mb-2">
          <span class="text-2xl">⚡</span>
          <div>
            <h2 class="text-xl font-bold">001 — First Team Meeting: The Night of the Awakening</h2>
            <p class="text-sm text-gray-400">February 7, 2026 — 00:47 to 01:33 UTC · Duration: 46 minutes · 7 decisions · 5 disagreements resolved</p>
          </div>
        </div>
