    const deptColors = { exec: 'purple', sales: 'emerald', marketing: 'pink', dev: 'blue', design: 'fuchsia', finance: 'yellow', product: 'teal', ops: 'red' };
    const deptNames = { exec: 'Executive', sales: 'Sales', marketing: 'Marketing', dev: 'Development', design: 'Design', finance: 'Finance', product: 'Product', ops: 'Operations' };

    function renderAgentGrid(filter = 'all') {
      const grid = document.getElementById('agentGrid');
      if (!grid) return;
      const agents = filter === 'all' ? agentData : agentData.filter(a => a.dept === filter);
      grid.innerHTML = agents.map(a => {
        const statusColor = a.status === 'active' ? 'bg-green-500' : 'bg-yellow-500';
        const borderColor = `border-${a.color}-500/30`;
        const hoverBorder = `hover:border-${a.color}-500`;
        return `
          <div class="agent-card bg-gray-800 rounded-xl border ${borderColor} ${hoverBorder} p-4 relative" onclick="openAgentDetail(${a.id})">
            <div class="status-dot ${statusColor}"></div>
            <div class="flex items-center mb-3">
              <img src="https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/${a.pokemonId}.png" class="pokemon-sprite mr-3" alt="${a.pokemon}">
              <div>
                <h4 class="font-bold text-${a.color}-400">${a.name}</h4>
                <p class="text-xs text-gray-400">${a.pokemon}</p>
              </div>
            </div>
            <p class="text-xs text-gray-400 mb-2">${a.role}</p>
            <div class="flex items-center justify-between">
              <span class="text-xs px-2 py-0.5 bg-${a.color}-500/20 text-${a.color}-400 rounded">${deptNames[a.dept]}</span>
              <div class="flex items-center">
                <div class="w-12 bg-gray-700 rounded-full h-1.5 mr-1"><div class="bg-${a.color}-500 h-1.5 rounded-full" style="width:${a.context}%"></div></div>
                <span class="text-xs text-gray-500">${a.context}%</span>
              </div>
            </div>
          </div>
        `;
      }).join('');
    }

    function filterAgents(dept) { renderAgentGrid(dept); }

    function openAgentDetail(id) {
      const a = agentData.find(ag => ag.id === id);
      if (!a) return;
      document.getElementById('agentConfigTitle').textContent = `${a.name} — ${a.pokemon}`;
      document.getElementById('agentConfigContent').innerHTML = `
        <div class="flex items-center mb-6">
          <img src="https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/${a.pokemonId}.png" class="w-24 h-24 mr-4" style="image-rendering:pixelated">
          <div>
            <h3 class="text-xl font-bold text-${a.color}-400">${a.name}</h3>
            <p class="text-gray-400">${a.role}</p>
            <p class="text-sm text-gray-500 mt-1">${a.pokemon} • ${deptNames[a.dept]}</p>
            <span class="inline-block mt-2 text-xs px-2 py-1 rounded ${a.status === 'active' ? 'bg-green-500/20 text-green-400' : 'bg-yellow-500/20 text-yellow-400'}">${a.status === 'active' ? '🟢 Active' : '🟡 Idle'}</span>
          </div>
        </div>
        <div class="bg-gray-700/50 rounded-lg p-4 mb-4">
          <h4 class="font-semibold mb-2 text-sm">Personality</h4>
          <p class="text-sm text-gray-300">${a.personality}</p>
        </div>
        <div class="grid grid-cols-2 gap-4 mb-4">
          <div class="bg-gray-700/50 rounded-lg p-4">
            <h4 class="font-semibold mb-2 text-sm">Skills</h4>
            <div class="flex flex-wrap gap-1">${a.skills.map(s => `<span class="text-xs px-2 py-1 bg-${a.color}-500/20 text-${a.color}-400 rounded">${s}</span>`).join('')}</div>
          </div>
          <div class="bg-gray-700/50 rounded-lg p-4">
            <h4 class="font-semibold mb-2 text-sm">Context Usage</h4>
            <div class="flex items-center"><div class="flex-1 bg-gray-700 rounded-full h-3 mr-3"><div class="bg-${a.color}-500 h-3 rounded-full" style="width:${a.context}%"></div></div><span class="font-bold">${a.context}%</span></div>
          </div>
        </div>
        <div class="space-y-3">
          <div class="bg-gray-700/50 rounded-lg p-3"><h4 class="text-xs font-semibold text-gray-400 mb-1">WORKS WITH</h4><p class="text-sm">${a.worksWith.join(', ')}</p></div>
          <div class="bg-gray-700/50 rounded-lg p-3"><h4 class="text-xs font-semibold text-gray-400 mb-1">SENDS TO</h4><p class="text-sm">${a.sendsTo.join(', ')}</p></div>
          <div class="bg-gray-700/50 rounded-lg p-3"><h4 class="text-xs font-semibold text-gray-400 mb-1">RECEIVES FROM</h4><p class="text-sm">${a.receivesFrom.join(', ')}</p></div>
        </div>
      `;
      openModal('agent-config');
    }

    // ===================== BOTTOM DEPARTMENT BAR =====================
    const deptPageMap = {
      home: 'dashboard',
      exec: 'strategy-hub',
      sales: 'sales-crm',
      marketing: 'marketing-campaigns',
      dev: 'dev-projects',
      design: 'marketing-brand',
      finance: 'finance-overview',
      product: 'plan-roadmap',
      ai: 'ai-team'
    };

    function switchDept(dept) {
      document.querySelectorAll('.dept-item').forEach(d => d.classList.remove('active'));
      const item = document.querySelector(`[data-dept="${dept}"]`);
      if (item) item.classList.add('active');
      showPage(deptPageMap[dept] || 'dashboard');
    }

    // ===================== CRM TABS =====================
    function switchCrmTab(tab, btn) {
      document.querySelectorAll('.crm-tab').forEach(t => { t.classList.remove('active'); });
      if (btn) btn.classList.add('active');
      // In a real app, this would filter leads by type
      console.log('Switched CRM to:', tab);
    }

    // ===================== ABOUT US LANGUAGE TOGGLE =====================
    function toggleAboutLang(lang) {
      const en = document.getElementById('aboutContentEn');
      const he = document.getElementById('aboutContentHe');
      const btnEn = document.getElementById('btnLangEn');
      const btnHe = document.getElementById('btnLangHe');
      if (lang === 'he') {
        en.classList.add('hidden'); he.classList.remove('hidden');
        btnEn.classList.remove('bg-indigo-500'); btnEn.classList.add('bg-gray-700');
        btnHe.classList.remove('bg-gray-700'); btnHe.classList.add('bg-indigo-500');
        document.getElementById('aboutTitle').textContent = 'אודותינו';
        document.getElementById('aboutSubtitle').textContent = 'המשימה, הצוות והחזון שלנו';
      } else {
        he.classList.add('hidden'); en.classList.remove('hidden');
        btnHe.classList.remove('bg-indigo-500'); btnHe.classList.add('bg-gray-700');
        btnEn.classList.remove('bg-gray-700'); btnEn.classList.add('bg-indigo-500');
        document.getElementById('aboutTitle').textContent = 'About Us';
        document.getElementById('aboutSubtitle').textContent = 'Our mission, team, and vision';
      }
    }

    // ===================== EXEC ACTION ITEMS FILTER =====================
    function filterExecActions(person) {
      document.querySelectorAll('.exec-action-card').forEach(card => {
        if (person === 'all' || card.dataset.person === person) {
          card.style.display = '';
        } else {
          card.style.display = 'none';
        }
      });
    }

    // ===================== QA BUG TRACKING =====================
    function addBug() { alert('Bug report form — coming soon!'); }
    function filterBugs() { console.log('Filtering bugs by project'); }

    // ===================== SYSTEM MONITOR =====================
    function refreshSystemStatus() { alert('System status refreshed!'); }

    // ===================== ABOUT TEAM GRID =====================
    function renderAboutTeamGrid() {
      const grid = document.getElementById('aboutTeamGrid');
      if (!grid) return;
      grid.innerHTML = agentData.map(a => `
        <div class="text-center bg-gray-700/50 rounded-lg p-2 hover:bg-gray-700 transition cursor-pointer" onclick="openAgentDetail(${a.id})">
          <img src="https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/${a.pokemonId}.png" class="w-12 h-12 mx-auto" style="image-rendering:pixelated" alt="${a.pokemon}">
          <p class="text-xs font-bold text-${a.color}-400 mt-1">${a.name}</p>
        </div>
      `).join('');
    }

    // ===================== P&L CHART =====================
    function initPnlChart() {
      const ctx = document.getElementById('pnlChart');
      if (!ctx) return;
      new Chart(ctx.getContext('2d'), {
        type: 'bar',
        data: {
          labels: ['Oct 2025', 'Nov 2025', 'Dec 2025', 'Jan 2026', 'Feb 2026'],
          datasets: [
            { label: 'Revenue', data: [1800, 2400, 2800, 3200, 3850], backgroundColor: '#22c55e', borderRadius: 4 },
            { label: 'Expenses', data: [2220, 3200, 3900, 4100, 4280], backgroundColor: '#ef4444', borderRadius: 4 }
          ]
        },
        options: {
          responsive: true, maintainAspectRatio: false,
          scales: { y: { grid: { color: '#374151' }, ticks: { color: '#9ca3af', callback: v => '$' + v } }, x: { grid: { display: false }, ticks: { color: '#9ca3af' } } },
          plugins: { legend: { labels: { color: '#9ca3af' } } }
        }
      });
    }

    // ===================== DASHBOARD LIVE FEED =====================
    function renderLiveFeed() {
      const feed = document.getElementById('liveFeedContainer');
      if (!feed) return;
      const activities = [
        { time: '10:05', icon: '💼', agent: 'STRIKER', action: 'Sending 20 Telegram outreach msgs', dept: 'sales' },
        { time: '10:02', icon: '📝', agent: 'LYRIX', action: 'Drafted blog post: SDK Integration Guide', dept: 'marketing' },
        { time: '09:58', icon: '🔧', agent: 'FORGE', action: 'Gateway memory optimization deployed', dept: 'dev' },
        { time: '09:45', icon: '📊', agent: 'CIPHER', action: 'Daily revenue report generated', dept: 'finance' },
        { time: '09:30', icon: '🔍', agent: 'SCOUT', action: 'Found 3 new SDK partnership opportunities', dept: 'sales' },
        { time: '09:15', icon: '🎨', agent: 'PIXEL', action: 'Weathero icon redesign v3 completed', dept: 'design' },
        { time: '09:00', icon: '🧠', agent: 'ULTRON', action: 'Morning strategy brief distributed', dept: 'exec' },
        { time: '08:45', icon: '🐛', agent: 'GLITCH', action: 'SDK v1.0.57 regression tests: 94% pass', dept: 'dev' },
      ];
      feed.innerHTML = activities.map(a => `
        <div class="flex items-center py-2 border-b border-gray-700/50 last:border-0">
          <span class="text-xs text-gray-500 w-12 flex-shrink-0">${a.time}</span>
          <span class="mr-2">${a.icon}</span>
          <span class="text-xs font-medium text-${deptColors[a.dept] || 'gray'}-400 mr-2">${a.agent}</span>
          <span class="text-xs text-gray-400 truncate">${a.action}</span>
        </div>
      `).join('');
    }

    // ===================== ENHANCED SHOWPAGE =====================
    const origShowPage = showPage;
    showPage = function(pageId) {
      origShowPage(pageId);
      // Update dept bar
      const deptForPage = {
        'dashboard': 'home', 'strategy-hub': 'exec', 'exec-actions': 'exec', 'about-us': 'exec', 'ecosystem': 'exec',
        'sales-crm': 'sales', 'sales-emails': 'sales',
        'marketing-campaigns': 'marketing', 'marketing-content': 'marketing', 'marketing-social': 'marketing', 'marketing-events': 'marketing', 'marketing-brand': 'marketing',
        'dev-projects': 'dev', 'dev-apps': 'dev', 'dev-websites': 'dev',
        'finance-overview': 'finance', 'partners-clients': 'finance', 'finance-invoices': 'finance', 'finance-expenses': 'finance', 'finance-pnl': 'finance',
        'plan-roadmap': 'product', 'competitor-intel': 'product', 'market-research': 'product',
        'ai-team': 'ai', 'ai-chat': 'ai', 'agent-logs': 'ai',
        'system-specs': 'home', 'system-logs': 'home', 'ai-config': 'home',
        'testing': 'dev', 'plan-testing': 'dev', 'qa-schedule': 'dev', 'dev-qa': 'dev',
        'plan-tasks': 'home', 'plan-schedule': 'home',
        'partners-accounts': 'sales', 'partners-support': 'sales',
        'resources': 'home'
      };
      const dept = deptForPage[pageId] || 'home';
      document.querySelectorAll('.dept-item').forEach(d => d.classList.remove('active'));
      const deptItem = document.querySelector(`[data-dept="${dept}"]`);
      if (deptItem) deptItem.classList.add('active');
    };

    // Init
    document.addEventListener('DOMContentLoaded', () => {
      initCharts();
      loadRecentActivity();
      updateTime();
      setInterval(updateTime, 1000);
      loadChatHistories();
      loadLeads();
      loadFinanceData();
      loadTestResults();
      loadDashboardData();
      loadClientData();
      renderAgentGrid();
      renderAboutTeamGrid();
      renderLiveFeed();
      initPnlChart();
    });
  
    // Integration Wizard selector
    function selectInteg(el, name) {
      document.querySelectorAll('.integ-item').forEach(i => i.classList.remove('integ-active'));
      el.classList.add('integ-active');
    }

    </script>
</body>
</html>
