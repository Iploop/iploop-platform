    <script>
    function filterDiscussionThreads(team) {
      document.querySelectorAll('.discussion-thread').forEach(t => {
        if (team === 'all' || t.dataset.teams.split(',').includes(team)) {
          t.style.display = '';
        } else {
          t.style.display = 'none';
        }
      });
    }
    function searchDiscussionThreads(query) {
      const q = query.toLowerCase();
      document.querySelectorAll('.discussion-thread').forEach(t => {
        if (!q || t.textContent.toLowerCase().includes(q)) {
          t.style.display = '';
        } else {
          t.style.display = 'none';
        }
      });
    }
    </script>

