          <button type="button" onclick="closeModal('add-lead')" class="px-4 py-2 bg-gray-700 rounded-lg">Cancel</button>
          <button type="submit" class="px-4 py-2 bg-emerald-500 rounded-lg hover:bg-emerald-600">Add Lead</button>
        </div>
      </form>
    </div>
  </div>

  <script>
    // Navigation
    function showPage(pageId) {
      document.querySelectorAll('.page').forEach(p => p.classList.remove('active'));
      document.querySelectorAll('.nav-item').forEach(n => n.classList.remove('active'));
      
      const page = document.getElementById('page-' + pageId);
      const navItem = document.querySelector(`[data-page="${pageId}"]`);
      
      if (page) page.classList.add('active');
      if (navItem) navItem.classList.add('active');
      
      // Close sidebar on mobile
      if (window.innerWidth < 1024) {
        toggleSidebar();
      }
    }
    
    function toggleSidebar() {
      const sidebar = document.getElementById('sidebar');
      const overlay = document.getElementById('overlay');
      sidebar.classList.toggle('open');
      overlay.classList.toggle('hidden');
    }
    
    function toggleNavGroup(btn) {
      const items = btn.nextElementSibling;
      const isHidden = items.classList.contains('hidden');
      
      // Close all other groups
      document.querySelectorAll('.nav-group-items').forEach(el => el.classList.add('hidden'));
      document.querySelectorAll('.nav-group-btn').forEach(el => el.classList.remove('expanded'));
      
      // Toggle this group
      if (isHidden) {
        items.classList.remove('hidden');
        btn.classList.add('expanded');
      }
    }
    
    // ===================== CONTENT FILTERING =====================
    function filterContent(type, btn) {
      // Update button states
      document.querySelectorAll('.content-tab').forEach(t => {
        t.classList.remove('bg-pink-500');
        t.classList.add('bg-gray-700');
      });
      btn.classList.remove('bg-gray-700');
      btn.classList.add('bg-pink-500');
      
      // Filter content cards
      document.querySelectorAll('.content-card').forEach(card => {
        if (type === 'all' || card.dataset.type === type) {
          card.style.display = '';
        } else {
          card.style.display = 'none';
        }
      });
    }
    
    // ===================== LOGS REFRESH =====================
    function refreshLogs() {
      // Placeholder for live log refresh
      console.log('Refreshing logs...');
    }
    
    // ===================== COMMAND PALETTE =====================
    
    const commands = [
      // Navigate — Main
      { name: 'Command Center', icon: 'fa-chart-pie', action: () => showPage('dashboard'), category: 'Navigate' },
      { name: 'Strategy Hub', icon: 'fa-chess', action: () => showPage('strategy-hub'), category: 'Navigate' },
