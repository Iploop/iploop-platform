      { name: 'Social Calendar', icon: 'fa-share-nodes', action: () => showPage('marketing-social'), category: 'Navigate' },
      { name: 'Events', icon: 'fa-calendar-days', action: () => showPage('marketing-events'), category: 'Navigate' },
      { name: 'Brand Assets', icon: 'fa-palette', action: () => showPage('marketing-brand'), category: 'Navigate' },
      // Navigate — Dev
      { name: 'Dev Projects', icon: 'fa-folder-tree', action: () => showPage('dev-projects'), category: 'Navigate' },
      { name: 'Apps Portfolio', icon: 'fa-mobile-alt', action: () => showPage('dev-apps'), category: 'Navigate' },
      { name: 'Websites', icon: 'fa-globe', action: () => showPage('dev-websites'), category: 'Navigate' },
      // Navigate — Product
      { name: 'Roadmap', icon: 'fa-road', action: () => showPage('plan-roadmap'), category: 'Navigate' },
